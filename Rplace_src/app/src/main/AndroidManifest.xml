                android:name="android.support.PARENT_ACTIVITY"
                android:theme="@style/Theme.AppCompat.NoActionBar"
                android:value="com.gaurav.android.rplace.RemoveGateway" />
        </activity>
        <activity
            android:name=".removePreviousData"
            android:label="@string/title_activity_remove_previous_data"
            android:parentActivityName=".RemoveGateway"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
