<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gaurav.android.rplace" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Rplace"
            android:label="@string/app_name"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.AppCompat.NoActionBar" >

        </activity>
        <activity
            android:name=".add_student"
            android:label="@string/title_activity_add_student"
            android:parentActivityName=".Rplace"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.gaurav.android.rplace.Rplace" />
        </activity>
        <activity
            android:name=".RemoveStudent"
            android:label="@string/title_activity_remove_student"
            android:parentActivityName=".Rplace"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.gaurav.android.rplace.Rplace" />
        </activity>
        <activity
            android:name=".Display"
            android:label="@string/title_activity_display"
            android:parentActivityName=".Rplace"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.gaurav.android.rplace.Rplace" />
        </activity>
        <activity
            android:name=".GUIView"
            android:label="@string/title_activity_guiview"
            android:parentActivityName=".Display"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.gaurav.android.rplace.Display" />
        </activity>
        <activity
            android:name=".add_main_gateway"
            android:label="@string/title_activity_add_main_gateway"
            android:parentActivityName=".Rplace"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.gaurav.android.rplace.Rplace" />
        </activity>
        <activity
            android:name=".RemoveGateway"
            android:label="@string/title_activity_remove_gateway"
            android:parentActivityName=".Rplace"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.gaurav.android.rplace.Rplace" />
        </activity>
        <activity
            android:name=".ViewGateway"
            android:label="@string/title_activity_view_gateway"
            android:parentActivityName=".Rplace"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:screenOrientation="landscape"
                android:value="com.gaurav.android.rplace.Rplace" />
        </activity>
        <activity
            android:name=".AdvancedSettings"
            android:label="@string/title_activity_advanced_settings"
            android:parentActivityName=".Rplace"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:screenOrientation="landscape"
                android:value="com.gaurav.android.rplace.Rplace" />
        </activity>
        <activity
            android:name=".HomeFullscreenActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_home_fullscreen"
            android:theme="@style/FullscreenTheme" >
        </activity>
        <activity
            android:name=".company_add"
            android:label="@string/title_activity_company_add"
            android:parentActivityName=".add_main_gateway"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.gaurav.android.rplace.add_main_gateway" />
        </activity>
        <activity
            android:name=".add_previous_data"
            android:label="@string/title_activity_add_previous_data"
            android:parentActivityName=".add_main_gateway" >
