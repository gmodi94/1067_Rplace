                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.gaurav.android.rplace.RemoveGateway" />
        </activity>
        <activity
            android:name=".ViewData"
            android:label="@string/title_activity_view_data"
            android:parentActivityName=".ViewGateway"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.gaurav.android.rplace.ViewGateway" />
        </activity>
        <activity android:name="org.achartengine.GraphicalActivity" />
        <activity
            android:name=".SplashActivity"
            android:label="@string/title_activity_splash"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"
                    />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AboutApp"
            android:label="@string/title_activity_about_app"
            android:parentActivityName=".AdvancedSettings"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.gaurav.android.rplace.AdvancedSettings"
               />
        </activity>
    </application>

</manifest>
