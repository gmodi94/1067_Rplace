                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.gaurav.android.rplace.add_main_gateway" />
        </activity>
        <activity
            android:name=".RemoveCompany"
            android:label="@string/title_activity_remove_company"
            android:parentActivityName=".RemoveGateway"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
